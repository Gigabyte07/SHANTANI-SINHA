/* traffic light code
Creator: SHANTANI
*/



int ledG=11;
int ledY=12;
int ledR=13;
void setup()
{
  pinMode(11,OUTPUT);
  pinMode(12,OUTPUT);
  pinMode(13,OUTPUT);
}
void loop()
{
  digitalWrite(11,HIGH);
  delay(1);
  digitalWrite(11,LOW);
  delay(1);
  digitalWrite(13,HIGH);
  delay(1);
  digitalWrite(13,LOW);
  delay(1);
  digitalWrite(12,HIGH);
  delay(1);
  digitalWrite(12,LOW);
  delay(1);
}

